{
  "name": "testcafe-cucumber-demo",
  "version": "0.0.1",
  "description": "An example integration of TestCafe into CucumberJS",
  "author": "helen-dikareva",
  "scripts": {
    "debug": "node --inspect=1337 --debug-brk --nolazy node_modules/cucumber/bin/cucumber-js --tags @debug",
    "test": "./node_modules/.bin/cucumber-js.cmd",
    "test-report": "./node_modules/.bin/cucumber-js.cmd --format json:./reports/report.json --tags @test"
  },
  "dependencies": {
    "cucumber": "^4.0.0",
    "testcafe": "^0.18.6",
    "base64-img": "^1.0.4"
  }
}
